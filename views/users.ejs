<!doctype html>
<html>

<head>
    <title>Users List</title>
    <link rel="stylesheet" type="text/css" href="/main.css" />
</head>

<body>
    <h1>Registered Users:</h1>
    <ul>
        <% availableUsers.forEach(function(user) { %>
            <li>First name:<%= user.first_name %>, Last Name: <%= user.last_name %>, Username: <%= user.username %>, Email: <%= user.email %></li>
        <% }); %>
    </ul>
</body>

</html>